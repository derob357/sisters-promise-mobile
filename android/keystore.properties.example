# Keystore configuration for release builds
# Copy this file to keystore.properties and fill in your values
# DO NOT commit keystore.properties to version control!

# Path to your release keystore file (relative to android directory)
storeFile=release.keystore

# Keystore password
storePassword=your_keystore_password

# Key alias in the keystore
keyAlias=your_key_alias

# Key password
keyPassword=your_key_password

# To generate a new keystore, run:
# keytool -genkeypair -v -storetype PKCS12 -keystore release.keystore -alias your_key_alias -keyalg RSA -keysize 2048 -validity 10000
